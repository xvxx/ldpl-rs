# from http://docs.ldpl-lang.org/procedure/

data:
    result is number

procedure:
    sub addTwoNumbers
        parameters:
            a is number
            b is number
            c is number
        procedure:
            in c solve a + b
    end sub

    # the variable result is initialized to 0 by default
    call addTwoNumbers with 4 5 result
    display "The result is: " result "." lf